{
  "name": "projection-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -c \"blue,green\" -n \"SERVER,FRONTEND\" \"bun run dev:server\" \"bun run dev:frontend\"",
    "dev:server": "bun --hot run server/index.ts",
    "dev:frontend": "bunx --bun vite",
    "build": "bun run build:frontend && bun run build:server",
    "build:frontend": "tsc -b && bunx --bun vite build",
    "build:server": "bun run ex.ts",
    "start": "NODE_ENV=production ./dist/server/my-app",
    "lint": "eslint .",
    "preview": "bunx --bun vite preview"
  },
